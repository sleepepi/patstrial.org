- @title = @page.name

- content_for :header, @title
= render "internal/tabs"
- content_for :sidebar, render("internal/sidebar")

- @page.reports.where(archived: false).each do |report|
  = render "reports/chart", report: report if report.report_type == "randomizations_by_site_by_month"
  = render "reports/table", report: report
