= form_with model: project, local: true do |form|
  = render "forms/horizontal/text_field", form: form, object: project, key: :name, autocomplete: "off", autocorrect: "off", autocapitalize: "none", spellcheck: "false"
  = render "forms/horizontal/text_field", form: form, object: project, key: :access_token, autocomplete: "off", autocorrect: "off", autocapitalize: "none", spellcheck: "false"
  = render "forms/horizontal/submit", form: form, object: project
