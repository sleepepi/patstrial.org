- @title = "Edit Member: #{@member.name_was}"

.container
  .row
    .col-sm-9.col-sm-push-3
      .color-container.dark-purple
        %h1.brand
          = link_to 'Members', editor_members_path
          &middot;
          = link_to @member.name_was, editor_member_path(@member)
          &middot;
          Edit

      .color-container
        = render 'form'

    .col-sm-3.col-sm-pull-9
      = render 'setup/menu'
